   "source": [
    "**1. Move zeros to end**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[2, 3, 4, -1, 0, 0, 0, 0, 0]\n"
     ]
    }
   ],
   "source": [
    "def move_zeros_end(nums):\n",
    "    c = nums.count(0)\n",
    "    nums[:] = [num for num in nums if num!=0]\n",
    "    nums.extend([0]*c)\n",
    "    return nums\n",
    "\n",
    "arr = [0,0,2,3,0,0,4,-1,0]\n",
    "res = move_zeros_end(arr)\n",
    "print(res)\n"
   ]
  }
